{
  "version": 2,
  "builds": [
    {
      "src": "app/__init__.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/__init__.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "app/__init__.py"
  },
  "regions": ["sin1"],
  "functions": {
    "app/__init__.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}